# Database
ConnectionStrings__Default=Host=localhost;Database=propertymanager;Username=postgres;Password=your_password

# JWT Configuration
Jwt__Secret=your-256-bit-secret-key-minimum-32-characters
Jwt__Issuer=http://localhost:5000
Jwt__Audience=http://localhost:4200
Jwt__ExpiryMinutes=60

# Email Configuration
Email__Provider=Smtp
Email__SmtpHost=localhost
Email__SmtpPort=1025
Email__FromAddress=noreply@property-manager.app
Email__FromName=Property Manager

# AWS S3 (for receipt storage - future use)
AWS__AccessKeyId=your_access_key
AWS__SecretAccessKey=your_secret_key
AWS__BucketName=property-manager-receipts
AWS__Region=us-east-1

# Environment
ASPNETCORE_ENVIRONMENT=Development
